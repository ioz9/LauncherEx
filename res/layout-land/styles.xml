<!-- Froyo theme -->
<RelativeLayout
		android:id="@+id/froyo_theme"
		android:visibility="gone" 
        android:layout_width="56dip"
        android:layout_height="fill_parent"
        android:layout_gravity="right|center"
        android:layout_alignParentRight="true"
        android:paddingLeft="2dip" >

		<RelativeLayout android:id="@+id/froyo_center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true" >

			<ImageView
	    			android:background="@drawable/froyo_center"
