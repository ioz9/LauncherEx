	    			android:layout_width="wrap_content" 
	    			android:layout_centerInParent="true"
	    			android:onClick="onButtonDialer" />

		</RelativeLayout>

		<RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_above="@id/froyo_center">

        	<ImageView
            		android:background="@drawable/froyo_right"
            		android:layout_marginTop="4dip"
            		android:paddingBottom="12dip"
        			android:paddingTop="12dip"
        			android:layout_width="64dip"
        			android:layout_height="64dip"
